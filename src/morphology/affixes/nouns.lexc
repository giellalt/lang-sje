! even-syllabic
! -------------

LEXICON N_EVEN ! giella
+N: EVENCASE ;  ! Sg Nom, EVENOBLIQUE
+N: N_EVEN_ILL ;
+N:^WG N_EVEN_WK ; ! CG here

LEXICON N_EVEN_O ! rássjo-variant rässjuj
+N: EVENCASE ;  ! Sg Nom, EVENOBLIQUE
+N: N_EVEN_ILL ; 
+N: N_EVEN_ILL_O ; ! allows rássjo-variant: rässjuj
+N:^WG N_EVEN_WK ; ! CG here

LEXICON N_EVEN_4 ! gonagassa
+N: EVENCASE_4 ;
+N:a N_EVEN_ILL ;
+N:a^WG N_EVEN_WK ; ! CG here

LEXICON EVENCASE_4
+Sg+Nom:^CDEL K ;
:a EVENOBLIQUE ;

LEXICON EVENCASE
+Sg+Nom:^UAUML    K ;
EVENOBLIQUE ;

LEXICON EVENOBLIQUE 
N_EVEN_ESS ; 
!:^WG^IJ%>j N_EVEN_J ;

LEXICON N_EVEN_NOCG
+N: EVENCASE ;
+N: N_EVEN_ILL ;
+N: N_EVEN_WK ; ! No CG here, ingen stadieväxling

LEXICON N_EVEN_ILL
+Sg+Ill:%>j^ILLE2AA K ; !V2 e->á in ill.sg, dim

LEXICON N_EVEN_ILL_O
+Sg+Ill+Use/NG:%>j^ILLO2U K ; !V2 o->u in ill.sg, dim

LEXICON N_EVEN_ESS
+Ess:^UAUML%>n K ;

LEXICON N_EVEN_WK
+Sg+Gen:  K ;
+Sg+Acc:%>v  K ;
+Sg+Ine:%>n K ;
+Sg+Ela+Err/Orth:%>s K ;
+Sg+Ela:%>st K ;
