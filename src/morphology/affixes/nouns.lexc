!+Abe:%>htaka K ; ! Informants S and Bj 
!+Abe:%>htak K ; ! Informants S and Bj 
+Pl+Nom:%>0  K ;
!:^WG^IJ%>j N_EVEN_J ;
:^IJ%>j N_EVEN_J ;

