!! !!!Even syllabled verbs

LEXICON V_EVEN_E !båhtet
+V:     V_EVEN_E_STR ;
+V:e^WG V_EVEN_E_WK ;

LEXICON V_EVEN_E_STR
+Ind+Prs+Sg3:á%> #;
!+Ind+Prs+Sg3+Err/Orth:a%> #; !just as an example, for possible later implementation
+Ind+Prs+Du1:i%>n #; 
+Ind+Prs+Du2:e%>behtin #;
+Ind+Prs+Du2:e%>bähtten #;
+Ind+Prs+Du3:e%> 3DUPRS ;
+Ind+Prs+Pl1:e%>p #;
+Ind+Prs+Pl2:e%>behtit #;
+Ind+Prs+Pl2:e%>bähttet #;
+Ind+Prs+Pl3:i%> #; 
+Ind+Prt+Sg1:i%>v #; 
+Ind+Prt+Sg2:i%> #; 
+Ind+Prt+Pl3:i%>n #; 

+Imprt+Du2:e%>n # ; 
+Imprt+Pl2:i%>t #;

+Inf:e%>t # ; 
+GerII:e%>min # ; !aka: +Actio+Ess ?
+PrfPrc:á%>m # ;
+PrsPrc:e%>j # ; 
+Vsubst:e%>m #; 

LEXICON V_EVEN_E_WK
+Ind+Prs+Sg1:%>v^V2E2AA #;
+Ind+Prs+Sg2:%>^V2E2AA #;
:^IJ%>j V_EVEN_E_WK_J ; !suffixes with -j-element that trigger e->i in V2

+Imprt+Sg2:%> #; 

+GerI:%>din # ;  
+ConNeg:%> # ; 
+Vgen:%> #;  
!+Sup:%>tjit # ;
+Pot:%>tj V_EVEN_POT ; 

LEXICON V_EVEN_E_WK_J
+Ind+Prt+Sg3: #;
+Ind+Prt+Du1:men #;
+Ind+Prt+Du2:den #;
+Ind+Prt+Du3: 3DUPRT ;
+Ind+Prt+Pl1:me #;
+Ind+Prt+Pl2:de #;



LEXICON V_EVEN_A !dahkat
+V:     V_EVEN_A_STR ;
+V:a^WG V_EVEN_A_WK ;

LEXICON V_EVEN_A_STR
+Ind+Prs+Sg3:á%> #;
+Ind+Prs+Du1:i%>n #; 
+Ind+Prs+Du2:a%>behtin #;
!+Ind+Prs+Du2:a%>bähtten #;
+Ind+Prs+Du3:a%> 3DUPRS ;
+Ind+Prs+Pl1:a%>p #;
+Ind+Prs+Pl2:a%>behtit #;
!+Ind+Prs+Pl2:a%>bähttet #;
+Ind+Prs+Pl3:i%> #; 
+Ind+Prt+Sg1:i%>v #; 
+Ind+Prt+Sg2:i%> #; 
+Ind+Prt+Pl3:i%>n #; 

+Imprt+Du2:e%>n # ; 
+Imprt+Pl2:i%>t #;

+Inf:a%>t # ; 
+GerII:a%>min # ;
+PrfPrc:a%>m # ;
+PrsPrc:e%>j # ; 
!+Vsubst:a%>m #; 

LEXICON V_EVEN_A_WK
+Ind+Prs+Sg1:%>v #;
+Ind+Prs+Sg2:%> #;
+Ind+Prt+Sg3:%>j #;
+Ind+Prt+Du1:%>jmen #;
+Ind+Prt+Du2:%>jden #;
+Ind+Prt+Du3:%>j 3DUPRT ;
+Ind+Prt+Pl1:%>jme #;
+Ind+Prt+Pl2:%>jde #;

+Imprt+Sg2:%> #; 

+GerI:%>din # ;  
!+Sup:%>tjit # ;
+ConNeg:%> # ; 
+Vgen:%> #;  
+Pot:%>tj V_EVEN_POT ;


LEXICON V_GILGIJ !special case for deviant gilgij-forms of gallgat
+V:	V_GILGIJ_FORMS ;

LEXICON V_GILGIJ_FORMS !see above
+Ind+Prt+Sg3:%>j #;
+Ind+Prt+Du1:%>jmen #;
+Ind+Prt+Du2:%>jden #;
+Ind+Prt+Du3:%>j 3DUPRT ;
+Ind+Prt+Pl1:%>jme #;
+Ind+Prt+Pl2:%>jde #;


LEXICON V_EVEN_O !viessot, årrot
+V:     V_EVEN_O_STR ;
+V:o^WG V_EVEN_O_WK ;

LEXICON V_EVEN_O_STR
+Ind+Prs+Sg3:u%> #; !should this be -u-?
+Ind+Prs+Du1:u%>n #; 
+Ind+Prs+Du2:o%>behtin #;
+Ind+Prs+Du2:o%>bähtten #;
+Ind+Prs+Du3:o%> 3DUPRS ;
+Ind+Prs+Pl1:o%>p #;
+Ind+Prs+Pl2:o%>behtit #;
+Ind+Prs+Pl2:o%>bähttet #;
+Ind+Prs+Pl3:u%> #; 
+Ind+Prt+Sg1:u%>v #; 
+Ind+Prt+Sg2:u%> #; 
+Ind+Prt+Pl3:u%>n #; 

+Imprt+Du2:o%>n # ; 
+Imprt+Pl2:u%>t #;

+Inf:o%>t # ; 
+GerII:o%>min # ;
+PrfPrc:u%>m # ;
+PrsPrc:o%>j # ; 
+Vsubst:o%>m #; 

LEXICON V_EVEN_O_WK
+Ind+Prs+Sg1:%>v #;
+Ind+Prs+Sg2:%> #;
+Ind+Prt+Sg3:%>j #;
+Ind+Prt+Du1:%>jmen #;
+Ind+Prt+Du2:%>jden #;
+Ind+Prt+Du3:%>j 3DUPRT ;
+Ind+Prt+Pl1:%>jme #;
+Ind+Prt+Pl2:%>jde #;

+Imprt+Sg2:%> #; 

+GerI:%>din # ;  
!+Sup:%>tjit # ;
+ConNeg:%> # ; 
+Vgen:%> #;  
+Pot:%>tj V_EVEN_POT ; 


LEXICON V_EVEN_Å !hållåt, bårråt
+V:     V_EVEN_Å_STR ;
+V:å^WG V_EVEN_Å_WK ;

LEXICON V_EVEN_Å_STR
+Ind+Prs+Sg3:á%> #;
+Ind+Prs+Du1:i%>n #; 
+Ind+Prs+Du2:å%>behtin #;
!+Ind+Prs+Du2:å%>bähtten #;
+Ind+Prs+Du3:å%> 3DUPRS ;
+Ind+Prs+Pl1:å%>p #;
+Ind+Prs+Pl2:å%>behtit #;
!+Ind+Prs+Pl2:å%>bähttet #;
+Ind+Prs+Pl3:i%> #; 
+Ind+Prt+Sg1:i%>v #; 
+Ind+Prt+Sg2:i%> #; 
+Ind+Prt+Pl3:i%>n #; 

+Imprt+Du2:e%>n # ; 
+Imprt+Pl2:i%>t #;

+Inf:å%>t # ; 
+GerII:å%>min # ;
+PrfPrc:å%>m # ;
+PrsPrc:e%>j # ; !-åt cognate with -at verbs in saL/saN, which also get -ej
!+Vsubst:å%>m #; 

LEXICON V_EVEN_Å_WK
+Ind+Prs+Sg1:%>v #;
+Ind+Prs+Sg2:%> #;
+Ind+Prt+Sg3:%>j #;
+Ind+Prt+Du1:%>jmen #;
+Ind+Prt+Du2:%>jden #;
+Ind+Prt+Du3:%>j 3DUPRT ;
+Ind+Prt+Pl1:%>jme #;
+Ind+Prt+Pl2:%>jde #;

+Imprt+Sg2:%> #; 

+GerI:%>din # ;  
!+Sup:%>tjit # ;
+ConNeg:%> # ; 
+Vgen:%> #;  
+Pot:%>tj V_EVEN_POT ; 



!POT forms for V_EVEN_
LEXICON V_EVEN_POT !
+Sg1:a%>v #;
+Sg2:a%> #;
+Sg3:a%> #;
+Sg3+Use/NG:%> #;
+Du1:i%>n #; 
+Du2:ä%>hppen #;
+Du3:e%>ba #;
+Pl1:i%>p #;
+Pl2:i%>hppit #;
+Pl3:i%> #; 



