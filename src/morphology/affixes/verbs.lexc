!! !!!contracted verbs

LEXICON V_CONTR !välldut, tjerrut
+V+Ind+Prs+Sg1:%>v #;
+V+Ind+Prs+Sg2:%> #;
+V+Ind+Prs+Sg3:%>ja #;
+V+Ind+Prs+Du1:%>jin #; 
+V+Ind+Prs+Du2:%>bähttin #;
+V+Ind+Prs+Du3:%>ba #;
+V+Ind+Prs+Pl1:%>p #;
+V+Ind+Prs+Pl2:%>bihttit #;
+V+Ind+Prs+Pl3:%>ji #; 

+V+Ind+Prt+Sg1:%>jiv #; 
+V+Ind+Prt+Sg2:%>ji #; 
+V+Ind+Prt+Sg3:%>j #;
+V+Ind+Prt+Du1:%>jmin #;
+V+Ind+Prt+Du2:%>jdin #;
+V+Ind+Prt+Du3:%>j 3DUPRT ;
+V+Ind+Prt+Pl1:%>jme #;
+V+Ind+Prt+Pl2:%>jde #;
+V+Ind+Prt+Pl3:%>jin #; 

+V+Imprt+Sg2:%> #; 
!+V+Imprt+Du2:%>hten # ; 
!+V+Imprt+Pl2:%>httit #;

+V+Inf:%>t # ; 
+V+GerII:%>min # ;
+V+PrfPrc:%>m # ;
+V+ConNeg:%> # ; 
+V+PrsPrc:%>jäddje # ; 
!+V+Vsubst:%>m #; 
!+V+GerI:%>din # ;  
!+V+Sup:%>tjit # ;
!+V+Vgen:%> #;  

!V_CONTR POT forms -- check!
+V+Pot:%>tj V_CONTR_POT ;

!POT forms for V_CONTR
LEXICON V_CONTR_POT !
+Sg1:a%>v #;
+Sg2:a%> #;
+Sg3:a%> #;
+Sg3+Use/NG:%> #;
+Du1:i%>n #; 
+Du2:ä%>hppen #;
+Du3:e%>ba #;
+Pl1:i%>p #;
+Pl2:i%>hppit #;
+Pl3:i%> #; 






!! !!!Auxiliaries

LEXICON LE !!= * @CODE@ 
lä+V+Ind+Prs: LEPRS ; 
lä+V+Ind+Prt: LEPRT ; 
lä+V+Ind+Pot: LEPOT ;
lä+V+PrfPrc:lam # ; ! lä (conneg) + *am (prfprc allomorph of *lä*)
lä+V+Inf:årrot # ; !infinitive
lä+V+ConNeg:lä # ; !connegative

LEXICON LEPRS !!= * @CODE@ 
+Sg1:läv # ; 
+Sg1+Use/NG:lev # ; 
+Sg2:lä # ;  
+Sg2+Use/NG:le # ;  
+Sg2:l # ; !enclitic (only attaches to vowel-final preceding word)
+Sg3:lä # ; 
+Sg3:l # ; !enclitic (only attaches to vowel-final preceding word)
!+Sg3:le # ; 
+Du1:lin # ; 
!+Du1:len # ; 
+Du2:lihpen # ; 
+Du2:lähppen # ;  
+Du3:läbá # ; 
+Pl1:läp # ; 
+Pl1:lip # ; 
!+Pl1:lep # ; 
+Pl2:lihpit # ; 
+Pl2:lehpit # ; 
+Pl3:lä # ;
+Pl3:l # ; !enclitic (only attaches to vowel-final preceding word)

LEXICON LEPRT !!= * @CODE@ 
+Sg1:lijjiv    # ; 
+Sg1:lidjiv    # ;  
+Sg2:lijji    # ; 
+Sg2:lidji    # ;  
+Sg3:lij    # ; 
+Du1:lijmen # ; 
+Du1:liejme # ; 
+Du2:lijden # ;
+Du2:liejde # ; 
+Du3:lij 3DUPRT ;
+Du3:liej 3DUPRT ; 
+Pl1:lijme # ; 
+Pl1:liejme # ; 
+Pl2:lijde # ;
+Pl2:liejde # ; 
+Pl3:lidjin    # ;  
+Pl3:lijjin    # ; 
+Pl3:lin    # ; 

LEXICON LEPOT
+Sg1:lidtjav # ; 
+Sg2:lidtja # ; 
+Sg3:lidtj # ; 
+Du1:lidtjin # ; 
+Du2:lidtjabehtin # ; 
+Du3:lidtjaba # ; 
+Du3:lidtjaban # ; 
+Pl1:lidtjap # ;  
+Pl2:lidtjabihtit # ; 
+Pl3:lidtji # ; 


